import java.io.File;


public class Filedemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		File file =new File("F:\\FILE.txt");
		file.mkdirs();
		if(file.exists())
		{
			System.out.println(file.exists());
		}
		else {
			System.out.println(file.exists());
		}
	}

}
